.float_box {
  position: fixed;
  bottom: 100rpx;
  right: 30rpx;
  text-align: center;
}

.float_main {
  width: 80rpx;
  height: 80rpx;
  display: flex;
  overflow: hidden;
  align-items: center;
  justify-content: center;
  margin-top: 18rpx;
  background: #ffffffc4;
  border-radius: 50%;
  box-shadow: 0 0 8rpx rgba(0, 0, 0, 0.1);
}

.float_img {
  width: 40rpx;
  height: 40rpx;
}
