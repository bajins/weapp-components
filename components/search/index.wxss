.wrapper {
  width: 100vw;
  background: #fff;
  padding: 10rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
}

.seabox_bar {
  width: calc(100% - 80rpx);
  height: 60rpx;
  display: flex;
  align-items: center;
  border-radius: 30rpx;
  background: #f5f5f5;
  padding-left: 32rpx;
  margin: 10rpx 0rpx;
}

.icon {
  margin-right: 20rpx;
}

.input {
  width: 100%;
  height: 60rpx;
  font-size: 28rpx;
  margin-right: 10rpx;
}

.activity_seabtn {
  font-size: 28rpx;
  margin-left: 20rpx;
  color: rgb(140, 155, 167);
  text-align: center
}
